<%= form_for(@typeio) do |f| %>
  <% if @typeio.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@typeio.errors.count, "error") %> prohibited this typeio from being saved:</h2>
      <ul>
      <% @typeio.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :username %><br>
  </div>
  <div class="field">
    <%= f.label :body %><br>
    <%= f.text_area :body %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


